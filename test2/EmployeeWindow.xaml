<Window x:Class="test2.EmployeeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test2" xmlns:viewmodels="clr-namespace:test2.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:EmployeeViewModel}"
        mc:Ignorable="d"
        Title="EmployeeWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Projects"  Command="{Binding ProjectsCommand}" Width="200" />
            <Button Content="Leave Requests" Command="{Binding LeaveRequestsCommand}" Width="200"/>
            <Button Content="Log out" Command="{Binding LogoutCommand}" Width="200"/>
        </StackPanel>
    </Grid>
</Window>
