<Window x:Class="test2.AdministratorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:test2"
        mc:Ignorable="d"
        Title="AdministratorWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Projects" Width="200" Click="Projects_Click"/>
            <Button Content="Leave Requests" Width="200" Click="LeaveRequests_Click"/>
            <Button Content="Employes" Width="200" Click="Employes_Click"/>
            <Button Content="Approval Requests" Width="200" Click="ApprovalRequests_Click"/>
            <Button Content="Log out" Width="200" Click="Logout_Click"/>
        </StackPanel>
    </Grid>
</Window>
